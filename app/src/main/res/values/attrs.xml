<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ShimmerFrameLayout">
        <attr name="auto_start" format="boolean"/>
        <attr name="base_alpha" format="float"/>
        <attr name="duration" format="integer"/>
        <attr name="repeat_count" format="integer"/>
        <attr name="repeat_delay" format="integer"/>
        <attr name="repeat_mode" format="enum">
            <enum name="restart" value="1"/>
            <enum name="reverse" value="2"/>
        </attr>
        <attr name="angle" format="enum">
            <enum name="cw_0" value="0"/>
            <enum name="cw_90" value="90"/>
            <enum name="cw_180" value="180"/>
            <enum name="cw_270" value="270"/>
        </attr>
        <attr name="dropoff" format="float"/>
        <attr name="fixed_width" format="dimension"/>
        <attr name="fixed_height" format="dimension"/>
        <attr name="intensity" format="float"/>
        <attr name="relative_width" format="float"/>
        <attr name="relative_height" format="float"/>
        <attr name="shape" format="enum">
            <enum name="linear" value="0"/>
            <enum name="radial" value="1"/>
        </attr>
        <attr name="tilt" format="float"/>
    </declare-styleable>


    <declare-styleable name="LinearGradientTextView4">
        <attr name="lineNumber" format="integer"></attr>
        <attr name="showTime" format="integer"></attr>
        <attr name="textColor" format="color"></attr>
        <attr name="showStyle">
            <enum name="UNIDIRECTION" value="0"></enum>
            <enum name="TWOWAY" value="1"></enum>
        </attr>

    </declare-styleable>
    <declare-styleable name="ShimmerView">
        <attr name="reflectionColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="RoundAngleImageView">
        <attr name="roundWidth" format="dimension" />
        <attr name="roundHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="RoundImageView">
        <attr name="borderRadius" format="dimension" />
        <attr name="type" format="integer" />
        <attr name="lineWidth" format="dimension" />
        <attr name="triangleTop" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ProgressView">
        <attr name="colorType" format="integer" />
        <attr name="backColor" format="color"/>
        <attr name="strokeWidth" format="dimension" />
        <attr name="smallSize" format="dimension" />
        <attr name="bigSize" format="dimension" />
    </declare-styleable>

</resources>